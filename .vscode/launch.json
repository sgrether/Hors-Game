    "version": "0.2.0",
    "configurations": [
        {
            "name": "Horsin Around",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${horsin-around}/bin/godot.windows.editor.dev.x86_64.exe",
            // Change the arguments below for the project you want to test with.
            // To run the project instead of editing it, remove the "--editor" argument.
            "args": [ "--editor", "--path", "D:\\Godot Stuff\\horsin-around" ],
            "stopAtEntry": false,
            "cwd": "${horsin-around}",
            "environment": [],
            "console": "internalConsole",
            "visualizerFile": "${horsin-around}/platform/windows/godot.natvis",
            "preLaunchTask": "build"
          }
    ]
}